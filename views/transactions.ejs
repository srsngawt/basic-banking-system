    <br>
    <br>
    <h1>Transaction History</h1>

    <center>
        <table border="0" cellspacing="0">
            <tr>
                <th>Sender</th>
                <th>Receiver</th>
                <th>Transfer Amount</th>
            </tr>
            <% transactions.forEach(function(customer){ %>
                <tr>
                    <td><%= customer.sender %></td>
                    <td><%= customer.receiver %></td>
                    <td><%= customer.tAmount %></td>
                </tr>

            <% }) %>
        </table>
        <br>
        <hr width="49.6%">
